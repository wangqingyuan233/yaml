proxy-providers: 
  all: 
    type: http 
    url: "https://hello.stgod.com/clash/proxies" 
    path: hello.stgod.com/provider.yaml 
    interval: 1200
    health-check: 
      enable: true 
      interval: 60 
      url: http://www.gstatic.com/generate_204 
  east:
    type: http
    url: "https://hello.stgod.com/clash/proxies?c=CN,HK,TW,JP,KR,SG"
    path: ./proxy/east.yaml
    interval: 1200
    health-check:
      enable: true
      interval: 60
      url: http://www.gstatic.com/generate_204
